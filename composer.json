{
    "name": "uisits/ui",
    "description": "A package to scaffold UI for UIS-ITS laravel applications.",
    "type": "library",
    "keywords": ["skeleton", "its", "uis", "uis-its"],
    "authors": [
        {
            "name": "Chinwal Prasad",
            "email": "pchin3@uis.edu"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/console": "^9.0|^10.9",
        "illuminate/filesystem": "^9.0|^10.9",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "pestphp/pest": "^1.0|^2.6",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.0|^10.1",
        "orchestra/testbench": "^7.24|^8.5"
    },
    "autoload": {
        "psr-4": {
            "Uisits\\Ui\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uisits\\Ui\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
